{
  "name": "solital/solital",
  "description": "Solital is a framework for php projects. Contains a route system, template, ORM and API",
  "keywords": [
    "framework",
    "php",
    "framework-php"
  ],
  "license": "MIT",
  "require": {
    "php": ">= 7.2",
    "ext-json": "*",
    "solital/katrina": "0.2.*",
    "psr/container": "1.*",
    "psr/http-factory": "1.*",
    "psr/simple-cache": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Solital\\": "app/"
    },
    "files": [
      "app/Core/Course/Functions/helpers.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "composer dump-autoload -o"
    ],
    "post-install-cmd": [
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  }
}
