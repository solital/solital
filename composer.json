{
  "name": "solital/solital",
  "description": "Solital is a framework for php projects. Contains a route system, template, ORM and API",
  "license": "MIT",
  "homepage": "https://solital.github.io/docs-v2/",
  "keywords": [
    "framework",
    "php",
    "framework-php",
    "psr-16",
    "psr-14",
    "psr-11",
    "psr-4",
    "orm",
    "api-rest",
    "solital-framework",
    "solital",
    "orm-php-framework"
  ],
  "require": {
    "php": "^7.4|^8.0",
    "solital/core-tests": "2.0.0",
    "slowprog/composer-copy-file": "^0.3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Solital\\": "app/"
    },
    "files": [
      "config.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "SlowProg\\CopyFile\\ScriptHandler::copy",
      "composer dump-autoload -o"
    ],
    "post-install-cmd": [
      "SlowProg\\CopyFile\\ScriptHandler::copy",
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "SlowProg\\CopyFile\\ScriptHandler::copy",
      "composer dump-autoload -o"
    ]
  },
  "extra": {
    "copy-file": {
      "vendor/solital/core-tests/src/Resource/Helpers/": "app/Helpers/System",
      "vendor/solital/core-tests/src/Console/Components/templates/Controller.php": "app/Components/Controller/",
      "vendor/solital/core-tests/vinci": "vinci"
    }
  }
}