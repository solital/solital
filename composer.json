{
  "name": "solital/solital",
  "description": "Solital is a framework for php projects. Contains a route system, template, ORM and API",
  "license": "MIT",
  "homepage": "https://solital.github.io/docs-v1/",
  "require": {
    "php": ">= 7.2",
    "solital/core": "1.0.0-beta",
    "solital/katrina": "1.0.0-beta1"
  },
  "keywords": [
    "framework",
    "php",
    "framework-php"
  ],
  "autoload": {
    "psr-4": {
      "Solital\\": "app/"
    },
    "files": [
      "app/Functions/helpers.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "composer dump-autoload -o"
    ],
    "post-install-cmd": [
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  }
}